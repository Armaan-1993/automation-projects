{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "../out-tsc/e2e",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "target": "es2018",
    "types": ["jasmine", "node"],
    "moduleResolution": "node"
  }
}
